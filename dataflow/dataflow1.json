				"          flowTime,",
				"          pressure,",
				"          temperature,",
				"          rawValue = pulseCount,",
				"          intermediateValue,",
				"          primaryFactor,",
				"          waterVaporFactor,",
				"          volumeAccum,",
				"          energyAccum,",
				"          miscValue,",
				"          validate,",
				"          isSummer,",
				"          runTime,",
				"          chartLabelId,",
				"          time,",
				"          offset,",
				"          mass,",
				"          Zratio,",
				"          hydrocarbonDewpoint,",
				"          uncorrVolAccum,",
				"          gqId,",
				"          gqTimeReverse,",
				"          gqLastModTime,",
				"          gqHistoryTypeCode,",
				"          rawValueAccum,",
				"          volumeRatio,",
				"          adjustmentCodeName",
				"     )) ~> sink3",
				"split1@pulse sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     filePattern:'readingDyPulse[n]',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          objName,",
				"          orgName,",
				"          localTime,",
				"          timeFormat,",
				"          userName,",
				"          editStateName,",
				"          unitProfileName,",
				"          dqInfoName,",
				"          volume,",
				"          energy,",
				"          flowTime,",
				"          pressure = absolutePressure,",
				"          temperature,",
				"          rawValue = pulseCount,",
				"          intermediateValue,",
				"          primaryFactor,",
				"          waterVaporFactor,",
				"          volumeAccum,",
				"          energyAccum,",
				"          miscValue,",
				"          validate,",
				"          isSummer,",
				"          runTime,",
				"          chartLabelId,",
				"          time,",
				"          offset,",
				"          mass,",
				"          Zratio,",
				"          hydrocarbonDewpoint,",
				"          uncorrVolAccum,",
				"          gqId,",
				"          gqTimeReverse,",
				"          gqLastModTime,",
				"          gqHistoryTypeCode,",
				"          rawValueAccum,",
				"          volumeRatio,",
				"          adjustmentCodeName",
				"     )) ~> sink4"
			]
		}
	}
